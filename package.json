{
  "name": "aliha-mix",
  "version": "0.1.0",
  "description": "Assets compiler for Marusia Templates",
  "main": "index.js",
  "scripts": {
    "development": "cross-env NODE_ENV=development webpack --config=src/config/webpack.config.js --mode=development",
    "dev": "npm run development",
    "production": "cross-env NODE_ENV=production webpack --config=src/config/webpack.config.js --mode=production",
    "prod": "npm run production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliakseyenkaIhar/aliha-mix.git"
  },
  "keywords": [
    "WordPress",
    "webpack",
    "assets"
  ],
  "author": " <aliha.devs@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/AliakseyenkaIhar/aliha-mix/issues"
  },
  "homepage": "https://github.com/AliakseyenkaIhar/aliha-mix#readme",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "core-js": "^3.10.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "dotenv": "^8.2.0",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss-loader": "^5.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "webpack": "^5.29.0",
    "webpack-cli": "^4.6.0",
    "webpack-remove-empty-scripts": "^0.7.1"
  }
